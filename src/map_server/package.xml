<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format2.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="2">
    <name>map_server</name>
    <version>1.15.2</version>
    <description>
	    Refactored map server for ROS2 Navigation
    </description>
    <author>Brian Gerkey, Tony Pratkanis</author>
    <author>email="oregon.robotics.team@intel.com">Oregon Robotics Team</author>
    <maintainer email="oregon.robotics.team@intel.com">Oregon Robotics Team</maintainer>
    <license>Apache License 2.0</license>

	<buildtool_depend>ament_cmake</buildtool_depend>


    <depend>bullet</depend>
    <depend>nav_msgs</depend>
    <depend>std_msgs</depend>
    <depend>example_interfaces</depend>
    <depend>rclcpp</depend>
    <depend>sdl</depend>
    <depend>sdl-image</depend>
    <depend>yaml-cpp</depend>

	<export>
		<build_type>ament_cmake</build_type>
	</export>

</package>
