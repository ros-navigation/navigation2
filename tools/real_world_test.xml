<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4" main_tree_to_execute="RealWorldNavTree">

  <!-- Real Nav2 tree include from system -->
  <include ros_pkg="nav2_bt_navigator" path="behavior_trees/navigate_to_pose_w_replanning_and_recovery.xml"/>

  <!-- Custom tree that includes the Nav2 tree -->
  <BehaviorTree ID="RealWorldNavTree">
    <Sequence name="RealWorldNavigation">
      <CheckGoal goal="{goal}"/>
      <!-- Use the real Nav2 MainTree -->
      <SubTree ID="MainTree" goal="{goal}"/>
    </Sequence>
  </BehaviorTree>
</root>
